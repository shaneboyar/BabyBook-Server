    "db:reset": "yarn db:rollback:all; yarn db:migrate"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@google-cloud/storage": "^4.3.1",
    "@types/shortid": "^0.0.29",
    "apollo-server": "^2.10.1",
    "apollo-upload-client": "^12.1.0",
    "esm": "^3.2.25",
    "fast-crc32c": "^2.0.0",
    "graphql": "^14.6.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "shortid": "^2.2.15"
  }
}
