{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon -r esm src/index.ts",
    "db:rollback": "cd src/db; sequelize db:migrate:undo; cd ../..",
    "db:rollback:all": "cd src/db; sequelize db:migrate:undo:all; cd ../..",
